      WR2:
         SIZEOF(QUERY (pscoa <* USEDIN(SELF, 'PLANT_SPATIAL_CONFIGURATION.' + 'PLANT_SPATIAL_CONFIGURATION_ORGANIZATION_ASSIGNMENT.ITEMS')| (pscoa.role.name = 'plant owner'))) + SIZEOF(QUERY (pscpaoa <* USEDIN(SELF, 'PLANT_SPATIAL_CONFIGURATION.' + 'PLANT_SPATIAL_CONFIGURATION_PERSON_AND_ORGANIZATION_ASSIGNMENT.' + 'ITEMS')| (pscpaoa.role.name = 'plant owner'))) + SIZEOF(QUERY (pscpa <* USEDIN(SELF, 'PLANT_SPATIAL_CONFIGURATION.' + 'PLANT_SPATIAL_CONFIGURATION_PERSON_ASSIGNMENT.ITEMS')| (pscpa.role.name = 'plant owner'))) >= 1;
